{
  "expo": {
    "name": "ListHappens",
    "slug": "ListHappens",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/icon.png",
    "platforms": [
      "ios",
      "android"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.listhappens.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },	
    "android": {
      "package": "com.ListHappens.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },

    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "newArchEnabled": true
          },
          "ios": {
            "newArchEnabled": true,
            "deploymentTarget": "16.0"
          }
        }
      ],
      "@react-native-community/datetimepicker"
    ],

    "extra": {
      "eas": {
        "projectId": "9aacecdf-8b78-4daa-b11c-2a5aefe26693"
      }
    }
  }
}
